{
  "extends": "tslint-config-prettier",
  "rules": {
    "arrow-parens": true,
    "class-name": true,
    "eofline": true,
    "member-access": true,
    "semicolon": [false, "always"],
    "switch-default": true,
    "no-unused-variable": true,
    "arrow-return-shorthand": [true],
    "import-spacing": true,
    "newline-before-return": true
  }
}
